# NATS Message Bus Configuration
NATS_URL=nats://localhost:4222

# Logging Configuration
LOG_LEVEL=INFO

# Technical Analysis Agent Configuration
TECHNICAL_AGENT_NAME=technical-analysis-agent
TECHNICAL_DATA_WINDOW_SIZE=200
TECHNICAL_MIN_BARS_REQUIRED=50
TECHNICAL_PUBLISH_FREQUENCY=1

# RSI Configuration
TECHNICAL_RSI_PERIOD=14

# MACD Configuration  
TECHNICAL_MACD_FAST=12
TECHNICAL_MACD_SLOW=26
TECHNICAL_MACD_SIGNAL=9

# Bollinger Bands Configuration
TECHNICAL_BB_PERIOD=20
TECHNICAL_BB_STD=2.0

# Moving Averages Configuration
TECHNICAL_SMA_SHORT=20
TECHNICAL_SMA_LONG=50
TECHNICAL_EMA_SHORT=12
TECHNICAL_EMA_LONG=26

# Volume Indicators Configuration
TECHNICAL_VOLUME_SMA_PERIOD=20

# Development/Testing
TESTING=false
MOCK_DATA=false